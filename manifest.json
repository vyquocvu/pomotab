{
  "manifest_version": 2,

  "name": "Pomotab",
  "description": "Just open Pomoday.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "main.html"
  },
  "permissions": [
    "activeTab",
    "notifications"
  ],
  "background" : {
    "scripts" : ["background.js"]
  }
}